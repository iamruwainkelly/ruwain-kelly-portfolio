# AWS Configuration for Live Integration
# Copy this file to .env.local and add your AWS credentials

# AWS Credentials (Required for live integration)
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
VITE_AWS_REGION=us-east-1

# AWS Configuration Options
VITE_AWS_PROFILE=default
VITE_AWS_SESSION_TOKEN=

# Feature Flags
VITE_ENABLE_AWS_INTEGRATION=false
VITE_DEFAULT_TO_LIVE_MODE=false

# Development Settings
VITE_AWS_MOCK_DELAY=1000
VITE_AWS_DEBUG_MODE=false
